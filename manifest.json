{
	"name": "My Azure Partner Help Extension",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "You can simplify your work as partners by using this extension",
	"browser_action": {
		"default_icon": "img/icon128.png",
		"icons": { "16": "img/icon16.png",
				   "48": "img/icon48.png",
				   "128": "img/icon128.png" }
	  },
	"permissions": [
		"<all_urls>","webRequest","webRequestBlocking", "clipboardRead",  "storage"
	],
	"content_scripts": [
	{
		"matches": ["https://ms.portal.azure.com/*", "https://portal.azure.com/*"],
		"js": ["jquery-3.4.1.min.js", "script.js"],
		"run_at":"document_end"
		}
	],"background": {
		"scripts": ["jquery-3.4.1.min.js", "background.js"],
		"persistent": true
	}
}
